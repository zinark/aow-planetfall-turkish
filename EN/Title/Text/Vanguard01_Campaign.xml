      <Row ss:AutoFitHeight="0" ss:Height="30">
        <Cell>
          <Data ss:Type="String">EXPLORE_ELYSIUM_PARK_PART_03_INTRO_MESSAGE_HELP_BUTTON</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Ilia-5'e yardım edin</Data>
        </Cell>
