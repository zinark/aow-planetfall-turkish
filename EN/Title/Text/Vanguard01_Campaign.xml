        <Cell>
          <Data ss:Type="String">EXPLORE_ELYSIUM_PARK_QUEST_PART_03_CONFRONT</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION</Data>
        </Cell>
