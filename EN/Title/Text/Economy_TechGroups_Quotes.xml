          <Data ss:Type="String">SKILLGROUP_9_2_NAME</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Kozmite Ustalığı</Data>
        </Cell>
        <Cell ss:Index="6" ss:StyleID="s66" />
