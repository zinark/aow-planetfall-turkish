      <Row ss:Index="21" ss:AutoFitHeight="0">
        <Cell>
          <Data ss:Type="String">TECTONIC_SHIFT</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">NAME</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Tektonik Değişim</Data>
