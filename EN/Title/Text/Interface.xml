        <Cell>
          <Data ss:Type="String">MODAL_HOSTILE_ACTION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION_DEFENSIVE</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">{STR_targetPlayer} ile savunma anlaşmanızı bozmak üzeresiniz. Emin misin?</Data>
        </Cell>
        <Cell ss:Index="5">
