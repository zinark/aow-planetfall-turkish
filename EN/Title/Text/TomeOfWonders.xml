        <Cell>
          <Data ss:Type="String">OPERATION_STRENGTH</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">TEXT</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Çalışma Gücü [/hyperlink]</Data>
