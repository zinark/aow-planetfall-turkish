      <Row ss:AutoFitHeight="0" ss:Height="31.5">
        <Cell ss:StyleID="s65">
          <Data ss:Type="String">MIGRATE_OPERATION</Data>
        </Cell>
        <Cell ss:StyleID="s65">
          <Data ss:Type="String">CONFIRMATION</Data>
        </Cell>
        <Cell ss:StyleID="s65">
          <Data ss:Type="String">Bu koloniyi bir Sendika kolonisine taşımak istediğinizden emin misiniz?</Data>
        </Cell>
        <Cell ss:StyleID="s64" />
        <Cell ss:StyleID="s64" />
