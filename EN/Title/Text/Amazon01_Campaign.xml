      <Row ss:AutoFitHeight="0" ss:Height="30">
        <Cell>
          <Data ss:Type="String">AMA01_PRIZE_OF_THE_PREDATOR_COMPLETION_MESSAGE_PAGE_02</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Matron {STR_LeaderFirstName}, neyi bekliyorsun? Bu iğrençliğe bir son verin!</Data>
