        <Cell>
          <Data ss:Type="String">SABOTAGE_REACTOR_ALPHA_QUEST_SPECIAL_OP_OBJECTIVE</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Dahili Sistem Aşırı Yükü Hack işlemi kullanıldı</Data>
        </Cell>
