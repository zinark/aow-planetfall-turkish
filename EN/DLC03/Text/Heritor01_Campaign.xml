      <Row ss:AutoFitHeight="0">
        <Cell>
          <Data ss:Type="String">HER01_SEARCHING_FOR_CLUES_QUEST_OPERATION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Hatyl Es'Ran'ın Es'Teq eseriyle iletişim kurmasına izin verin.</Data>
        </Cell>
