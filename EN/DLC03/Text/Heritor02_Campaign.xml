      <Row ss:AutoFitHeight="0" ss:Height="45">
        <Cell>
          <Data ss:Type="String">HER02_CAPTURE_ILAI_JAREN_QUEST</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Ilai Jaren'i alt ederek onu Es'Teq rahiplerine teslim et. Sonra Es'Teq'in Yüce Lordlarından biriyle birleşebilir.</Data>
        </Cell>
