      <Row ss:AutoFitHeight="0" ss:Height="48">
        <Cell>
          <Data ss:Type="String">HIVE_QUEEN_EFFIGY</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Koloniye {HappinessIncrease} ve bu koloninin etki alanı içindeki Kir'Ko birimlerine {MoraleBonus} verir.</Data>
        </Cell>
      </Row>
      <Row ss:AutoFitHeight="0" ss:Height="30">
