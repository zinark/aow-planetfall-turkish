        <Cell>
          <Data ss:Type="String">OATH01_BREW_ELIXIR_BACKDOOR_QUEST_RESEARCH</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">DESCRIPTION</Data>
        </Cell>
        <Cell>
          <Data ss:Type="String">Adding five ingredients in the right order shouldn't be too difficult, right?</Data>
        </Cell>
        <Cell ss:Index="5">
          <Data ss:Type="String">A research option description.</Data>
